                <eventDateTime standardDateTime="2020-02-15T14:08:00Z">2:08 pm, 15 February 2020</eventDateTime> <!-- ISO 8601 date-time formatting -->
                <agentType>human</agentType> <!-- May contain: "human" or "machine" -->
                <agent xml:id="GPP-00001748">Callaghan, Samantha</agent> <!-- ID may be subject to change in the CW -->
                <eventDescription>Creation of NAF for Mary Robinson.</eventDescription> <!-- Include a full description of the maintenance event - ie: note what was created/edited during the event -->
